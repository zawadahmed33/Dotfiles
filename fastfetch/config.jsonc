{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "source": "~/.config/fastfetch/eosmall.txt",
    "padding": {
      "top": 2
    }
  },
  "general": {
    "multithreading": true
  },
  "display": {
    "separator": " ➜ "
  },
  "modules": [
    {
      "type": "title",
        "color": {
          "user": "32",
          "at": "34",
          "host": "34"
        }
    },
    "separator",
    {
      "key": " OS",
      "keyColor": "magenta",
      "type": "os"
    },
    {
      "key": " Machine",
      "keyColor": "cyan",
      "type": "host"
    },
    {
      "key": " Kernel",
      "keyColor": "magenta",
      "type": "kernel"
    },
    {
      "key": " Packages",
      "keyColor": "cyan",
      "type": "packages"
    },
    {
      "key": " Uptime",
      "keyColor": "magenta",
      "type": "uptime"
    },
    {
      "key": " Shell",
      "keyColor": "cyan",
      "type": "shell"
    },
    {
      "key": " Terminal",
      "keyColor": "magenta",
      "type": "terminal"
    },
    {
      "key": " Window Manager",
      "keyColor": "cyan",
      "type": "wm"
    },
    "separator",
    {
      "type": "custom",
      "format": "\u001b[31m  \u001b[32m  \u001b[33m  \u001b[34m  \u001b[35m  \u001b[36m "
    }
  ]
}
